{
  "$schema": "https://raw.githubusercontent.com/frysee/fthelper/main/generator/schema/generator.json",
  "generators": [
    {
      "display": "test",
      "type": "copy",
      "input": {
        "type": "directory",
        "mode": "single",
        "paths": ["{{ .current }}", "{{ .template }}", "test"]
      },
      "output": {
        "type": "directory",
        "mode": "single",
        "paths": ["{{ .current }}", "{{ .freqtrade }}", "test"]
      }
    },
    {
      "type": "bash",
      "commands": ["echo", "hello"],
      "withSudo": false
    }
  ]
}
